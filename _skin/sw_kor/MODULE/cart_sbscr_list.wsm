			<td>{{$화폐단위전}}{{$상품가격}}{{$화폐단위후}}</td>
			<td class="qty">{{$구매수량}}</td>
			<td class="tal">
				<p>기간 : {{$배송기간}}</p>
				<p>요일 : {{$배송요일}}</p>
				<p>주기/횟수 : {{$배송주기}}</p>
